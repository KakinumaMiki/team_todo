mixin team_form(action, method, team, err)
  form.box(action=action, method="post")
    _csrf
    _method=method

    - if(err)
      ul.errors
        each error in err.errors
          li= error.message

    div
      label(for='name') チーム名
      input#name(name="name", value!=team.name)
      
    div.actions
      input(type="submit")
